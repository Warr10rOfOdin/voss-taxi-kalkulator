# Voss Taxi Calculator - Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit .env to git!

# Google Maps API Key
# Get from: https://console.cloud.google.com/google/maps-apis
# Required APIs: Maps JavaScript API, Places API, Directions API
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Firebase Configuration
# Get from: https://console.firebase.google.com/ → Project Settings → General → Your apps
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX
VITE_FIREBASE_DATABASE_URL=https://your-project-default-rtdb.region.firebasedatabase.app

# Tariff Editor Password (optional, defaults to 'Hestavangen11')
VITE_TARIFF_PASSWORD=your_admin_password_here
